{
  "name": "s3-storage-app",
  "version": "1.0.0",
  "description": "S3 хранилище с фронтендом",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": ["s3", "storage", "file-upload"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "aws-sdk": "^2.1490.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "better-sqlite3": "^11.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  }
}
